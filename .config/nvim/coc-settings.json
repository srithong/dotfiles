{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "html",
    "javascript",
    "javascriptreact",
    "json",
    "markdown",
    "typescript",
    "typescript.tsx",
    "typescriptreact",
    "yaml",
    "go",
    "dart"
  ],
  "snippets.userSnippetsDirectory": "~/.config/nvim/snippets",
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "trace.server": "verbose",
      "filetypes": ["go"]
    }
  }
}
